                          "Jack of Diamonds","Jack of Hearts","Ten of Spades","Ace of Clubs"};
            
            for(int a = 0; a <cardsInArray.length;a++)
            {
                cards.add(cardsInArray[a]);
            }
            System.out.println(cards);
            Collections.shuffle(cards);
            System.out.println(cards);
            
            String pCards[] = new String[6];
