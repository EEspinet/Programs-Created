            }
            else if((vTotal>pTotal)&&(vTotal>qTotal)&(vTotal>rTotal)&&(vTotal>sTotal)&&(vTotal>tTotal)&&(vTotal>uTotal)&&(vTotal>wTotal))
            {
                System.out.println("Congrats! V hand wins with " + vTotal + " points!!");
            }
            else if((wTotal>pTotal)&&(wTotal>qTotal)&&(wTotal>rTotal)&&(wTotal>sTotal)&&(wTotal>tTotal)&&(wTotal>uTotal)&&(wTotal>vTotal))
            {
                System.out.println("Congrats! W hand wins with " + wTotal + " points!!");
            }
            else
            {
                System.out.println("There has beena a tie! No Winners!!");
            }
        }
