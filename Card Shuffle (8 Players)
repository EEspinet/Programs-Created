                    }
                    else if(P.equals("Six"))
                    {
                        pTotal+=5;
                    }
                    else if(P.equals("Five"))
                    {
                        pTotal+=4;
                    }
                    else if(P.equals("Four"))
                    {
                        pTotal+=3;
                    }
                    else if(P.equals("Three"))
                    {
                        pTotal+=2;
                    }
