            String wCards[] = new String[6];
            
            int pCounter = 0;
            int pIndex = 0;
            for (Iterator<String> iterator = cards.iterator(); iterator.hasNext();) 
            {
                if(pCounter >5)
                {
                    break;
                }
                else
                {
                      String stringP = iterator.next();
                      pCards[pIndex] = stringP;
                      iterator.remove();
                      pIndex +=1;
                      pCounter+=1;
                }
            }
            
            int qCounter = 0;
            int qIndex = 0;
            for(Iterator<String> iterator2 = cards.iterator();iterator2.hasNext();)
