//Card shuffle with 8 players!!

package card.shuffle.with.pkg8.players;

import java.util.ArrayList;
import java.util.Collections;
import java.util.Iterator;
import java.util.List;

/**
 * @Enrique F. Espinet
 */
public class CardShuffleWith8Players {

    public static void main(String[] args) {
        int randomNum = (int)(Math.random()*8 + 1);//creates a random number between 1-8. this is used to calculate who will win
        if((randomNum>=1) && (randomNum<=3))//if between 1 to 3, the dealer will win
        {
            String pCards[] = {"Ace of Spades","Queen of Diamonds", "King of Hearts", "Ace of Clubs","Ten of Spades","Queen of Hearts"};
            //a predetermined array of cards that will let the dealer always win..
            List<String> cards = new ArrayList<String>();//creates a list that will store each value in the cards array
            String cardsInArray[] = {"Four of Hearts","Ten of Hearts","Nine of Spades","Five of Spades",  
                          "Seven of Diamonds","Deuce of Diamonds","Six of Diamonds","Deuce of Spades",  
                          "Three of Clubs","Ace of Diamonds","Jack of Spades","Ten of Diamonds",
                          "Nine of Clubs","Seven of Clubs","Six of Clubs","Deuce of Clubs",   
                          "Three of Spades","Three of Hearts","Ace of Hearts","King of Spades",  
                          "Seven of Hearts","Four of Diamonds","Three of Diamonds", 
                          "Ten of Clubs","Five of Clubs","Five of Hearts","Deuce of Hearts",  
                          "Eight of Clubs","Queen of Spades","King of Diamonds","Nine of Hearts",  
                          "Eight of Diamonds","Four of Spades","Eight of Hearts",  
                          "Six of Hearts","Queen of Clubs","Seven of Spades",  
                          "Jack of Clubs","Four of Clubs","Six of Spades","Eight of Spades",  
                          "Five of Diamonds","King of Clubs","Nine of Diamonds",
                          "Jack of Diamonds","Jack of Hearts"};
            //the unshuffled deck of cards
            for(int a = 0; a <cardsInArray.length;a++)
            {
                //we get each cards from the array and store it into the list
                cards.add(cardsInArray[a]);
            }
            Collections.shuffle(cards);//we shuffle the deck so its random..
            
            /*
            we create 7 arrays for the remaining playrs, this will hold their cards
            */
            String qCards[] = new String[6];
            String rCards[] = new String[6];
            String sCards[] = new String[6];
            String tCards[] = new String[6];
            String uCards[] = new String[6];
            String vCards[] = new String[6];
            String wCards[] = new String[6];
            
            int qCounter = 0;//a counter used for later
            int qIndex = 0;//a coutner used for indexing
            for(Iterator<String> iterator2 = cards.iterator();iterator2.hasNext();)
            {//an iterator loop that will go through each card of the list until their is no more card to go through
                if(qCounter >5)
                {//once each player ha reached 6 cards, we will break from the innermost for loop
                    break;
                }
                else
                {
                    String stringQ = iterator2.next();//we grab the item from list and put it into a string
                    qCards[qIndex] = stringQ;//we put the string into its array
                    iterator2.remove();//we remove the item we put into the array in order to prevent multiple cards from being dealed
                    qIndex+=1;//we increment the indexing for the array
                    qCounter+=1;//we increment the card counter for the player
                }
            }
            
            //same as above..
            int rCounter = 0;
            int rIndex = 0;
            for(Iterator<String> iterator3 = cards.iterator();iterator3.hasNext();)
            {
                if(rCounter > 5)
                {
                    break;
                }
                else
                {
                    String stringR = iterator3.next();
                    rCards[rIndex] = stringR;
                    iterator3.remove();
                    rIndex+=1;
                    rCounter+=1;
                }
            }
            
            //same as above
            int sCounter = 0;
            int sIndex = 0;
            for(Iterator<String> iterator4 = cards.iterator();iterator4.hasNext();)
            {
                if(sCounter > 5)
                {
                    break;
                }
                else
                {
                    String stringS = iterator4.next();
                    sCards[sIndex] = stringS;
                    iterator4.remove();
                    sIndex+=1;
                    sCounter+=1;
                }
            }
            
            //same as above..
            int tCounter = 0;
            int tIndex = 0;
            for(Iterator<String> iterator5 = cards.iterator();iterator5.hasNext();)
            {
                if(tCounter>5)
                {
                    break;
                }
                else
                {
                    String stringT = iterator5.next();
                    tCards[tIndex] = stringT;
                    iterator5.remove();
                    tIndex+=1;
                    tCounter+=1;
                }
