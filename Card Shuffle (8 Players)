                String[] tempP = tempPhraseP.split(" ");//we split up the word using "spaces" and store it into a array
               
                int tempCounterP = 0;//a counter used for later..
                for(int b = 0; b < 2; b++)
                {
                    String P = tempP[tempCounterP];//we grab the item from the index and put it into another string
